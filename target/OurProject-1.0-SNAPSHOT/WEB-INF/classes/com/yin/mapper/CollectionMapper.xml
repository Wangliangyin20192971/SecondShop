<?xml version="1.0" encoding="UTF8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yin.mapper.CollectionMapper">

    <insert id="addCollection" parameterType="com.yin.domain.Collection">
        insert into sc_collection(user_id,goods_id,goods_name,goods_message,goods_price,goods_type)
        values (#{userId},#{goodsId},#{goodsName},#{goodsMessage},#{goodsPrice},#{goodsType})
    </insert>

</mapper>